<?xml version="1.0" encoding="utf-8"?>
<resources>


    <!-- Used for Tipi questionnaire -->

    <!-- Possible answers for Profile activity Spinners -->
    <string-array name="genders">
        <item>select</item>
        <item>Female</item>
        <item>Male</item>
        <item>Other</item>
    </string-array>

    <array name="genders_icons">
        <item>@drawable/empty</item>
        <item>@drawable/icon_female</item>
        <item>@drawable/icon_male</item>
        <item>@drawable/icon_questionmark</item>
    </array>


    <string-array name="ages">
        <item>select</item>
        <item>Under 18</item>
        <item>18–24</item>
        <item>25–29</item>
        <item>30–39</item>
        <item>40–49</item>
        <item>50–59</item>
        <item>60–69</item>
        <item>70+</item>
    </string-array>

    <string-array name="education">
        <item>select</item>
        <item>School (GCSE or similar)</item>
        <item>School (A-Levels, vocational, or similar)</item>
        <item>University Degree</item>
        <item>Advanced Degree (MA, PhD, etc.)</item>
    </string-array>

    <string-array name="language">
        <item>French</item>
        <item>Breton</item>
        <item>Basque</item>
        <item>Corse</item>
    </string-array>

</resources>
